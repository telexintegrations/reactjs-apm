{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "outDir": "dist",
    "declaration": true,
    "declarationMap": true,
    "emitDeclarationOnly": false,
    "skipLibCheck": true,
    "strict": true,
    "module": "NodeNext",
    "target": "ES6",
    "jsx": "react-jsx",
    "moduleResolution": "nodenext"
  },
  "include": ["src"],
  "exclude": ["src/__tests__"]
}
